{#- @var \SoureCode\Component\Menu\Model\MenuView menu -#}
{#- @var \SoureCode\Component\Menu\Model\MenuItemView item -#}
{% extends 'soure_code_menu.html.twig' %}

{%- block list -%}
    {%- set list_attributes = {'class': 'nav flex-column'} -%}
    {{ parent() }}
{%- endblock list -%}

{%- block list_item -%}
    {%- set list_item_attributes = {'class': 'nav-item'} -%}
    {{ parent() }}
{%- endblock list_item -%}

{%- block text -%}
    {%- set text_attributes = {'class': 'nav-link'} -%}
    {{ parent() }}
{%- endblock text -%}

{%- block link -%}
    {%- set link_attributes = {'class': 'nav-link'} -%}
    {{ parent() }}
{%- endblock link -%}

{%- block icon -%}
    {%- set icon_attributes = {'class': 'fa fa-fw'} -%}
    {{ parent() }}
{%- endblock icon -%}
